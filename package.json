{
  "private": true,
  "type": "module",
  "scripts": {
    "cb": "composer update && bun update",
    "build": "bun zz && vite build",
    "build:ssr": "bun zz && vite build && vite build --ssr",
    "dev": "bun zz && vite",
    "dev:ssr": "bun zz && vite build && vite build --ssr",
    "format": "prettier --write resources/",
    "format:check": "prettier --check resources/",
    "lint": "eslint . --fix",
    "fix": "./vendor/bin/fixr --dirty && bun run format && bun run lint",
    "analyse": "./vendor/bin/phpstan analyse",
    "analyse:baseline": "./vendor/bin/phpstan analyse --generate-baseline",
    "prod": "bun zz && vite build && vite build --ssr",
    "zz": "php artisan ziggy:generate --types"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@inertiajs/vue3": "^2.0.12",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.3",
    "@types/ziggy-js": "^1.8.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vueuse/core": "^13.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "laravel-echo": "^2.1.6",
    "laravel-vite-plugin": "^1.3.0",
    "lucide": "^0.515.0",
    "lucide-vue-next": "^0.516.0",
    "postcss-import": "^16.1.1",
    "pusher-js": "^8.4.0",
    "reka-ui": "^2.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue": "^3.5.16",
    "ziggy-js": "^2.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/runtime-core": "^3.5.16",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "semantic-release": "^24.2.5",
    "typescript-eslint": "^8.34.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "tw-animate-css": "^1.3.4",
    "vue-tsc": "^2.2.10"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.43.0",
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.10",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ],
      "type-enum": [
        2,
        "always",
        [
          "breaking",
          "feat",
          "fix",
          "refactor",
          "docs",
          "wip",
          "chore",
          "style",
          "test"
        ]
      ]
    }
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
